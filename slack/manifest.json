{
    "display_information": {
        "name": "sortes",
        "description": "Q&A agent",
        "background_color": "#002b36"
    },
    "features": {
        "bot_user": {
            "display_name": "sortes",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "app_mentions:read",
                "im:history"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_mention"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}